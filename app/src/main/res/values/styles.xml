<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="android:background">@android:color/transparent</item>
        <!-- Customize your theme here. -->
    </style>

    <style name="SessionMenuTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <item name="android:background">@android:color/transparent</item>
        <!-- Customize your theme here. -->
    </style>

    <!--Fruit Ninja-->
    <style name="buttonStyle" parent="android:Widget.Holo.Button">
        <item name="android:background">@drawable/buttons</item>
        <item name="android:minWidth">120dp</item>
        <item name="android:minHeight">40dp</item>
    </style>

    <!--Brick Breaker-->
    <style name="AppBaseTheme" parent="android:Theme.Black">
        <!--
            Theme customizations available in newer API levels can go in
            res/values-vXX/styles.xml, while customizations related to
            backward-compatibility can go here.
        -->
    </style>

    <style name="GameTheme" parent="android:Theme.Black.NoTitleBar.Fullscreen">
        <!-- Using fullscreen Activity on API <11, and old style elements -->
    </style>


    <!--style name="Widget.ActionBar" parent="@android:style/Widget.Holo.Light.ActionBar.Solid.Inverse">
        <item name="android:background">@drawable/ab_background</item>



    </style>

    <style name="Widget.ActionBar.Transparent">
        <item name="android:background">@android:color/transparent</item>
    </style-->


</resources>
